#include <stdio.h>

//This Code Adds the Last and the First digits of a Number

int main() {
	int num, last, first, sum;
	printf("Enter any Number: ");
	scanf("%d", &num);
	last = num%10;
	while(num >= 10){ num = num/10;}
	first = num;
	
	sum = first + last;
	printf("The Sum of first and last digits is: %d", sum);
	
	
	return 0;
}
